GOOGLE_GENAI_USE_VERTEXAI= # true to use Vertex AI, otherwise leave blank
GOOGLE_CLOUD_PROJECT=   # Your Google Cloud project ID (Vertex AI only)
GOOGLE_CLOUD_LOCATION=  # Your Google Cloud region (Vertex AI only)